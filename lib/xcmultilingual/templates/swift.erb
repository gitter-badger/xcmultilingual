import Foundation

<% @multilingual.each do |table| -%>
public enum <%= table.name %>: String {
<% table.keys.each do |key| -%>
  case <%= key %> = "<%= key %>"
<% end -%>

  public func string() {
    return NSLocalizedString(rawValue, tableName: "<%= table.name %>", bundle: nil, comment: "")
  }
}

<% end %>
